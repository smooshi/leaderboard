<table>
  <thead>
  <tr>
    <th>Rank</th>
    <th>Name</th>
    <th>Score</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.rank %></td>
        <td><%= user.name %></td>
        <td><%= user.high_score %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
  <% end %>
  </tbody>
</table>